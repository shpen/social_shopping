<% if !defined? @params then @params = {} end %>
<%= if highlight then link_to "Clear", questions_path(@params.merge(tag: nil, page: nil)), class: "btn btn-info" end %>
<%= raw(tags.map { |tag| link_to(tag, questions_path(@params.merge(tag: tag, page: nil)), class: "tag btn btn-primary #{'active' if tag == highlight}") }.join("")) %>